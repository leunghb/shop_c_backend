<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="shop.demo.mapper.BUserMapper">
    <select id="bLogin" resultType="Integer">
        select count(*) from b_user where account = #{account} and password = #{password};
    </select>
</mapper>